{
  "agent": "op_mini",
  "name": "Opera Mini",
  "patterns": [
    {
      "include": "#(Opera)\\/[9|10]\\.[80|50|61].*Mini\\/(?P<version>(\\d+)\\.(\\d+)(?:\\.(\\d+))?)#",
      "exclude": "#(UC? ?Browser|UCWEB|U3)#",
      "brand": "Opera Mini",
      "mobile": true
    },
    {
      "include": "#(?P<brand>Opera Mini)\\/9\\.80.*(Mobi|J2ME)\\/(?P<version>(\\d+)\\.(\\d+)(?:\\.(\\d+))?)#",
      "mobile": true
    },
    {
      "include": "#(?P<brand>Opera Mini)\\/(?P<version>9)\\s#",
      "exclude": "#(UC? ?Browser|UCWEB|U3)#",
      "mobile": true
    },
    {
      "include": "#(OPiOS)/(?P<version>(\\d+).(\\d+).(\\d+))#",
      "exclude": "#(UC? ?Browser|UCWEB|U3)#",
      "mobile": true,
      "brands": "Opera Mini",
      "engine": "WebKit"
    }
  ],
  "brands": [
    "Opera Mini"
  ]
}